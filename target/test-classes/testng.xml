<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="TestSuite" parallel="tests" thread-count="2">
    <listeners>
        <listener class-name="com.demo.listeners.TestListener"/>
    </listeners>
    <test name="UI Tests">
        <packages>
            <package name="com.demo.ui"/>
        </packages>
    </test>
    <test name="API Tests">
        <packages>
            <package name="com.demo.api"/>
        </packages>
    </test>
</suite>
